<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<configSections>
		<sectionGroup name="PatternGroup">
			<section name="Patterns" type="System.Configuration.NameValueSectionHandler"/>
		</sectionGroup>
	</configSections>

	<startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
	
	<PatternGroup>
		<Patterns>
			<add key="SSN" value="\d{3}-\d{2}-\d{4}"/>
			<add key="VISA" value="\b4[0-9]{12}(?:[0-9]{3})?"/>
			<add key="MC" value="(?:5[1-5][0-9]{2}|222[1-9]|22[3-9][0-9]|2[3-6][0-9]{2}|27[01][0-9]|2720)[0-9]{12}"/>
			<add key="AMEX" value="3[47][0-9]{13}"/>
			<add key="Phone" value="\d{3}-\d{3}-\d{4}"/>
			<add key="EMail" value="[^@\s]+@[^@\s]+\.[^@\s]+"/>
			<add key="Zip Code" value="[0-9]{5}(?:-[0-9]{4})"/>
			<add key="State Code" value="(?-i:A[LKSZRAEP]|C[AOT]|D[EC]|F[LM]|G[AU]|HI|I[ADLN]|K[SY]|LA|M[ADEHINOPST]|N[CDEHJMVY]|O[HKR]|P[ARW]|RI|S[CD]|T[NX]|UT|V[AIT]|W[AIVY])"/>
		</Patterns>
	</PatternGroup>
</configuration>